<tr class="table__row" data-task-id="{{this.id}}">
	<td class="cell__order-index">
		<span>1</span>
	</td>
	<td class="cell__order-work">
		<p>{{this.task_id}}</p>
	</td>
	<td class="cell__work-price">
		<span>{{this.task_price}}</span>
	</td>
	<td class="cell__counter">
		<span class="text_bold @@isUser">{{this.amount}}</span>
		<div class="counter @@isAdmin">
			<span class="counter__minus counterMinusJs"></span>
			<span class="counter__value counterValueJs">{{this.amount}}</span>
			<span class="counter__plus counterPlusJs"></span>
		</div>
	</td>
	<td class="cell__total @@isEdit">
		<span class="text_bold">{{this.task_total}}</span>
		<a href="delete-service" class="delete-item @@isAdmin modalOpenJs" data-status-scroll="0">
			<svg>
				<use xlink:href="#close"></use>
			</svg>
		</a>
	</td>
	<td class="cell-right @@isEdit">
		<span>46,00</span>
		<span class="text_bold @@isUser">4</span>
		<div class="counter @@isAdmin">
			<span class="counter__minus counterMinusJs"></span>
			<span class="counter__value counterValueJs">4</span>
			<span class="counter__plus counterPlusJs"></span>
		</div>
		<span class="text_bold">1230,00</span>
		<a href="delete-service" class="delete-item @@isAdmin modalOpenJs" data-status-scroll="0">
			<svg>
				<use xlink:href="#close"></use>
			</svg>
		</a>
	</td>
</tr>